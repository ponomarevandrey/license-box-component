mixin pagination(classList)
  div(class=`${classList}`)
    a(href='#wq##' class='pagination__item') &laquo;
    a(href='##fd#' class='pagination__item') 1
    a(href='##s#' class='pagination__item pagination__item_active') 2
    a(href='###' class='pagination__item') 3
    a(href='#da##' class='pagination__item') 4
    a(href='#fd##' class='pagination__item') &raquo;
